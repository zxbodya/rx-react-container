{
  "name": "rx-react-container",
  "version": "0.6.0",
  "author": "Bogdan Savluk <savluk.bogdan@gmail.com>",
  "description": "Provides HoC component, and utilities to connect RxJS logic to React Component.",
  "keywords": [
    "react",
    "rxjs",
    "container",
    "isomorphic"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zxbodya/rx-react-container.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.12.0"
  },
  "main": "dist/rx-react-container.cjs.js",
  "module": "dist/rx-react-container.esm.js",
  "sideEffects": false,
  "scripts": {
    "lint": "eslint ./src/",
    "build": "rimraf dist && rollup -c",
    "cover": "jest --coverage",
    "test": "jest",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.25.0",
    "codecov.io": "^0.1.6",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^22.4.3",
    "prettier": "^1.12.1",
    "prop-types": "^15.6.1",
    "raf": "^3.3.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-test-renderer": "16.3.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.58.1",
    "rollup-plugin-babel": "^3.0.3",
    "rxjs": "^6.0.0"
  },
  "peerDependencies": {
    "babel-runtime": "^6.25.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "rxjs": "^6.0.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "./setupJest"
    ],
    "roots": [
      "src"
    ]
  },
  "dependencies": {
    "hoist-non-react-statics": "^2.5.5"
  }
